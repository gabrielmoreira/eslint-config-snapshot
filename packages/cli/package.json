{
  "name": "@eslint-config-snapshotter/cli",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "eslint-config-snapshotter": "dist/index.js"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "dependencies": {
    "@eslint-config-snapshotter/config": "workspace:*",
    "@eslint-config-snapshotter/core": "workspace:*",
    "@eslint-config-snapshotter/diff": "workspace:*",
    "@eslint-config-snapshotter/extract": "workspace:*",
    "@eslint-config-snapshotter/sampling": "workspace:*",
    "@eslint-config-snapshotter/snapshot": "workspace:*",
    "@eslint-config-snapshotter/workspace": "workspace:*",
    "fast-glob": "^3.3.3"
  }
}
