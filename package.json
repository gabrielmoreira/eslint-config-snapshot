{
  "name": "eslint-config-snapshotter",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.29.3",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "prepare": "husky",
    "build": "pnpm nx run-many -t build",
    "lint": "pnpm nx run-many -t lint",
    "typecheck": "pnpm nx run-many -t typecheck",
    "test": "pnpm nx run-many -t test"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@eslint/js": "^10.0.1",
    "@eslint/json": "^1.0.1",
    "@eslint/markdown": "^7.5.1",
    "@manypkg/get-packages": "^3.1.0",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "fast-glob": "^3.3.3",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "nx": "22.5.0",
    "picomatch": "^4.0.3",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vitest": "^4.0.8"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,mjs,cjs}": [
      "pnpm eslint --fix"
    ]
  }
}
