{
  "name": "eslint-config-snapshot",
  "version": "0.14.0",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/gabrielmoreira/eslint-config-snapshot"
  },
  "homepage": "https://github.com/gabrielmoreira/eslint-config-snapshot#readme",
  "bugs": {
    "url": "https://github.com/gabrielmoreira/eslint-config-snapshot/issues"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.29.3"
  },
  "packageManager": "pnpm@10.29.3",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "prepare": "husky",
    "build": "pnpm nx run-many -t build",
    "lint": "pnpm nx run-many -t lint",
    "typecheck": "pnpm nx run-many -t typecheck",
    "test": "pnpm nx run-many -t test",
    "cli:dev": "pnpm exec tsx packages/cli/src/index.ts",
    "changeset": "changeset",
    "release:version": "changeset version && node scripts/sync-root-version.mjs",
    "release:publish": "changeset publish",
    "release:run": "node scripts/run-publish-workflow.mjs",
    "release:run:watch": "node scripts/run-publish-workflow.mjs --watch"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@eslint/js": "^10.0.1",
    "@eslint/json": "^1.0.1",
    "@eslint/markdown": "^7.5.1",
    "@manypkg/get-packages": "^3.1.0",
    "@types/debug": "^4.1.12",
    "@types/node": "^24.10.1",
    "@types/picomatch": "^4.0.2",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.2",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-sonarjs": "^3.0.7",
    "eslint-plugin-unicorn": "^63.0.0",
    "fast-glob": "^3.3.3",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "nx": "22.5.0",
    "picomatch": "^4.0.3",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vitest": "^4.0.8"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,mjs,cjs}": [
      "pnpm eslint --fix"
    ]
  },
  "eslint-config-snapshot": {}
}
